{
  "functions": {
    "src/pages/api/websites/[websiteId]/learning/*.ts": {
      "maxDuration": 30
    },
    "src/pages/api/**/*.ts": {
      "maxDuration": 10
    }
  },
  "env": {
    "DATABASE_URL": "@database_url",
    "DIRECT_DATABASE_URL": "@direct_database_url",
    "APP_SECRET": "@app_secret"
  },
  "build": {
    "env": {
      "DATABASE_URL": "@database_url",
      "DIRECT_DATABASE_URL": "@direct_database_url"
    }
  },
  "headers": [
    {
      "source": "/api/track",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "POST, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type"
        }
      ]
    },
    {
      "source": "/sela-tracker.js",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ]
}
